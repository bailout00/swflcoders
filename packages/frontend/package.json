{
  "name": "@swflcoders/frontend",
  "main": "entry.js",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "start": "EXPO_UNSTABLE_LIVE_BINDINGS=false expo start -c",
    "android": "EXPO_UNSTABLE_LIVE_BINDINGS=false expo run:android",
    "ios": "EXPO_UNSTABLE_LIVE_BINDINGS=false expo run:ios",
    "web": "EXPO_UNSTABLE_LIVE_BINDINGS=false expo start --web",
    "test": "jest --watchAll",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.17",
    "@tamagui/config": "^1.132.20",
    "@tamagui/font-inter": "^1.132.20",
    "@tamagui/lucide-icons": "^1.132.20",
    "@tamagui/toast": "^1.132.20",
    "@tanstack/react-query": "^5.85.5",
    "babel-preset-expo": "~14.0.2",
    "burnt": "^0.12.2",
    "expo": "54.0.0-preview.6",
    "expo-build-properties": "~1.0.3",
    "expo-font": "~14.0.2",
    "expo-linking": "~8.0.3",
    "expo-router": "6.0.0-beta.5",
    "expo-splash-screen": "~31.0.4",
    "expo-status-bar": "~3.0.3",
    "expo-system-ui": "~6.0.2",
    "expo-web-browser": "~15.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.0",
    "react-native-reanimated": "^4.0.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.14.1",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "^0.4.1",
    "tamagui": "^1.132.20",
    "zustand": "^5.0.8"
